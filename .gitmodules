[submodule "zsh/.oh-my-zsh"]
	path = zsh/.oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh.git
[submodule "zsh/.zaw"]
	path = zsh/.zaw
	url = https://github.com/zsh-users/zaw.git
[submodule "tmux/.tmux/plugins/tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "scripts/.scripts"]
	path = scripts/.scripts
	url = ssh://git@bitbucket.org/CristianCantoro/dotscripts.git
[submodule "ssh/.ssh"]
	path = ssh/.ssh
	url = git@bitbucket.org:CristianCantoro/dotssh.git
[submodule "zsh/oh-my-zsh/custom/plugins/zsh-autosuggestions"]
	path = zsh/oh-my-zsh/custom/plugins/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions.git
[submodule "nvm/.nvm"]
	path = nvm/.nvm
	url = https://github.com/creationix/nvm.git
[submodule "bash/.bash_it"]
	path = bash/.bash_it
	url = https://github.com/Bash-it/bash-it.git
[submodule "zsh/oh-my-zsh/custom/themes/colorbira"]
	path = zsh/oh-my-zsh/custom/themes/colorbira
	url = git@github.com:CristianCantoro/colorbira-zsh-theme.git
[submodule "zsh/oh-my-zsh/custom/plugins/conda"]
	path = zsh/oh-my-zsh/custom/plugins/conda
	url = https://github.com/esc/conda-zsh-completion.git
[submodule "zsh/oh-my-zsh/custom/plugins/zsh-histdb"]
	path = zsh/oh-my-zsh/custom/plugins/zsh-histdb
	url = https://github.com/larkery/zsh-histdb
[submodule "bat/bat-extra"]
	path = bat/bat-extra
	url = https://github.com/eth-p/bat-extras
